      <h3>Index of Authors.</h3>
      <table class="table">
        <tr><th>ID</th><th>First Name</th><th>Last Name</th><th></th><th>Email</th><th>Website</th><th></th><th>Created At</th><th>Updated At</th></tr>
        <% @author_list.each do |author| %>
          <tr>
      <% author.attributes.each do |attribute, values| %>
        <td> <%= values %></td>
      <% end %>
      </tr>
    <% end %>
    </table>
